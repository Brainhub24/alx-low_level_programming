#include <stdio.h>
void check_password(void* password) {
 const char* correct_password = "secret";
  if (strcmp((char*)password, correct_password) == 0) {
    printf("Access granted\n");
  } else {
    printf("Access denied\n");
  }
}
int main(int argc, char** argv)
{
  if (argc != 2) {
    fprintf(stderr, "Usage: %s <password>\n", argv[0]);
    return 1;
  }
  void* password_ptr = argv + 2;
  check_password(password_ptr);
  return 0;
}
